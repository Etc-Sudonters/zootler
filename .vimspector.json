{
  "configurations": {
    "run": {
      "adapter": "vscode-go",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/cmd/zootlr/main.go",
        "args": [
          "-d",
          "${workspaceRoot}/data/",
          "-l",
          "${workspaceRoot}/data/logic"
        ],
        "mode": "debug",
        "dlvToolPath": "$GOPATH/bin/dlv"
      }
    },
    "test": {
      "adapter": "vscode-go",
      "configuration": {
        "request": "launch",
        "program": "${fileDirname}",
        "args": [],
        "mode": "test",
        "dlvToolPath": "$GOPATH/bin/dlv"
      }
    },
    "parser": {
      "adapter": "vscode-go",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/cmd/parser/",
        "args": [
          "-l",
          "${workspaceRoot}/data/logic",
          "-f",
          "kind=Check;region=LLR.*;name=.*Chickens"
        ],
        "mode": "debug",
        "dlvToolPath": "$GOPATH/bin/dlv"
      }
    }
  }
}
